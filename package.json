    "@microsoft.azure/autorest.java": "latest",
    "@microsoft.azure/autorest.modeler": "latest",
    "@microsoft.azure/autorest.nodejs": "latest",
    "@microsoft.azure/autorest.python": "latest",
    "@microsoft.azure/autorest.ruby": "latest",
